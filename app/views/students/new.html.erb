<h1>Student Form</h1>
<%= form_tag students_path do %>
  <form action="<%= students_path%>" method="post">
    <label for="">First name:</label>
    <%= text_field_tag :"first_name" %>

    <label for="">Last name:</label>
    <%= text_field_tag :"last_name" %>

    <%= submit_tag "Submit Student" %>
  </form>
<% end %>
