<h3>Student Form</h3>

<% if session[:form_params] %>
<%= session[:form_params] %>
<% end %>

<%= form_tag students_path do %>
  <label>First Name:</label>
  <%= text_field_tag :first_name %>
 
  <label>Last Name:</label>
  <%= text_field_tag :last_name %><br>
 
  <%= submit_tag "Submit Student" %>
<% end %>

<%= params.inspect %>
