<h1> Student Form </h1>
<%= form_tag students_path do %>
<label>Student Info</label><br></br>
<input type="text" id="first_name" name="student[first_name]" value="First Name"><br>
<input type="text" id="last_name" name="student[last_name]" value="Last Name"><br>
<input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <input type="submit" value="Submit Student">
<% end %>
