<html>
<head>
  <title>new student</title>
</head>
<body>
  Student Form
  <%= form_tag students_path do %>
    <%= text_field_tag :first_name %>
    <%= text_field_tag :last_name %>
    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    <%= submit_tag "Submit Student" %>
  <% end %>

  <%= session[:form_params] %>


</body>
</html>
