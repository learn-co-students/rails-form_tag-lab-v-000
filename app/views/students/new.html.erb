<h1>Student Form</h1>

<%= form_tag students_path do %>
  <label for="first_name">Students First Name: </label><br>
  <%= text_field_tag :first_name %><br>

  <label for="last_name">Students Last Name: </label><br>
  <%= text_field_tag :last_name %><br>

  <%= submit_tag "Submit Student"%>
<% end %>
