<h1> Add a Student </h1>

<h4> Student Form </h4>
<%= form_tag students_path do %>
  <p>
  <label for="student_first_name">First Name:</label>
  <%= text_field_tag :first_name %>
  </p>

  <p>
  <label for="student_last_name">Last Name:</label>
  <%= text_field_tag :last_name %>
  </p>

  <%= submit_tag "Submit Student" %>
<% end %>
