<h3>Student Form</h3>

<%= form_tag students_path do%>
  <label>Student First Name:</label><br>
  <input type="text" name="student[first_name]" id="first_name">

  <label>Student Last Name:</label><br>
  <input type="text" name="student[last_name]" id="last_name">

  <%= submit_tag "Submit Student"%>
<%end%>
