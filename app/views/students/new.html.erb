<h1>Student Form</h1>
<%= form_tag students_path do %>
  <label>First Name:</label><br>
  <%= text_field_tag :first_name, params[:first_name], id: 'first_name' %>
  
 
  <label>Last Name:</label><br>
  <%= text_field_tag :last_name, params[:last_name], id: 'last_name' %><br>
 
  <%= submit_tag "Submit Student" %>
<% end %>

<%= params.inspect %>