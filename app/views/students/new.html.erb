<h1>Student Form</h1>

<%= form_tag students_path do %>
  <%= text_field_tag :first_name, nil, id: "student[first_name]" %><br>

  <%= text_field_tag :last_name, nil, id: "student[last_name]" %><br>

  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>

  <%= submit_tag "Submit Student" %>
<% end %>

<%= params.inspect %>
