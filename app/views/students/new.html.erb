<h1>Student Form</h1>

<%= form_tag students_path do %>
 
<label>First Name:</label><br><input name="student[first_name]" id="first_name"/><br>

<label>Last Name:</label><br><input name="student[last_name]" id="last_name"/><br>

<%= hidden_field_tag :authenticity_token, form_authenticity_token %><input type="submit" value="Submit Student">

<% end %>