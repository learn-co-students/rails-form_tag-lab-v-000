<h3>Student Form</h3>

<%= form_tag students_path do %>

    <%= label_tag 'First Name'%>
    <%= text_field_tag :'first_name' %><br>

    <%= label_tag 'Last Name' %>
    <%= text_field_tag :'last_name' %>

    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    <%= submit_tag "Submit Student"%>
<% end %>