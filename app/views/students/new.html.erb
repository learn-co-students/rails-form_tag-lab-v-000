<h2>Student Form</h2>

<%= form_tag create_student_path do %>
	<label>First Name</label>
	<%= text_field_tag 'first_name', name: 'student[first_name]' %>

	<label>Last Name</label>
	<%= text_field_tag 'last_name', name: 'student[last_name]' %>

 	<%= submit_tag "Submit Student" %>

 	<%= @params %>
<% end %>