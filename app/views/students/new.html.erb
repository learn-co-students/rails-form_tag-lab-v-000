<h1>Student Form</h1>
<%= form_tag students_path do %>
  <label>First Name:</label><br>
  <input id="first_name" name="student[first_name]" type="text" /><br>

  <label>Last Name:</label><br>
  <input id="last_name" name="student[last_name]" type="text" /><br>

  <%= submit_tag "Submit Student" %>
<% end %>

<%= params.inspect %>
