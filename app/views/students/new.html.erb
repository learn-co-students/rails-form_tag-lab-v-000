<label>Student Form</label>
  <%= form_tag students_path do %> <!--form_tag gives path for the post request-->
  <label>First name:</label>
  <%= text_field_tag :first_name %><br>
  <label>Last name:</label><br>
  <%= text_field_tag :last_name %><br>

  <%= submit_tag "Submit Student Info" %>
<% end %>
